{
  "name": "React-redux-app",
  "version": "1.0.0",
  "description": "React application",
  "main": "index.js",
  "scripts": {
    "build:lib": "babel src/server --out-dir lib/server",
    "build:umd": "webpack --config webpack/webpack.config.dev.js",
    "build:umd:min": "webpack --config webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "postinstall": "npm run build",
    "clean": "rimraf lib public/assets",
    "server": "better-npm-run server",
    "server:dev": "better-npm-run server:dev",
    "start": "npm run server",
    "start:dev": "npm run server:dev"
  },
  "betterScripts": {
    "server": {
      "command": "node lib/server/index.js",
      "env": {
        "NODE_ENV": "production",
        "MIN_EXT": ".min"
      }
    },
    "server:dev": {
      "command": "node devStart.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yury-dymov/habr-app.git"
  },
  "keywords": [
    "React",
    "universal",
    "isomorphic",
    "redux",
    "redux-oauth",
    "babel",
    "eslint"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.14.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt-nodejs": "0.0.3",
    "better-npm-run": "^0.1.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "crypto": "^1.0.1",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.9",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^15.0.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "~15.1.0",
    "react-redux": "^4.4.8",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "6.7.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "6.7.2",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.24.0",
    "detect-node": "^2.0.3",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.10.1",
    "prop-types": "^15.6.1",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^3.0.6",
    "rimraf": "2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
